{
  "schemaVersion": 1,
  "id": "discordfab",
  "version": "${version}",
  "name": "DiscordFab",
  "description": "Discord and Fabric server link",
  "authors": [
    "hansi132",
    "ItsIlya"
  ],
  "contact": {
    "repo": "https://github.com/KiloCraft/DiscordFab"
  },
  "license": "MIT",
  "environment": "server",
  "entrypoints": {
    "server": [
      {
        "adapter": "kotlin",
        "value": "io.github.hansi132.discordfab.DiscordFabMod"
      }
    ]
  },
  "depends": {
    "fabricloader": ">=0.9.2+build.206",
    "fabric": ">=${fabric_api}",
    "fabric-language-kotlin": "*",
    "minecraft": ">=${minecraft}",
    "luckperms": ">=${luckperms}"
  }
}

